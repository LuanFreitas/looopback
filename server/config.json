{
  "restApiRoot": "/api",
  "host": "api.mobradio.com.br",
  "port": 3002,
  "url": "https://api.mobradio.com.br/",
  "httpMode": false,
  "certConfig": {
    "path": "/etc/letsencrypt/live/api.mobradio.com.br/",
    "key": "privkey.pem",
    "cert": "cert.pem",
    "ca": [],
    "requestCert": false,
    "rejectUnauthorized": false
  },
  "remoting": {
    "context": false,
    "rest": {
      "handleErrors": false,
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false
  }
}
